{
  "name": "@swiftwc/ui",
  "version": "0.0.1-dev.8",
  "description": "Elegant SwiftUI-inspired web components for ambitious web apps and web extensions.",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": {
    "name": "The SwiftWC/UI Authors",
    "url": "https://github.com/swiftwc/ui/graphs/contributors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swiftwc/ui.git",
    "directory": "packages/ui"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "components",
    "ui",
    "tailwind",
    "swift"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "default": "./dist/components/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "default": "./dist/client/index.js"
    }
  },
  "bin": {
    "swiftwc": "./dist/index.js"
  },
  "scripts": {
    "build:ts": "tsc"
  }
}
